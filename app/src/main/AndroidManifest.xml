        <activity android:name=".Url_visit" />
        <activity android:name=".Profile_Page" />
        <activity android:name=".Resources" />
        <activity android:name=".Profile_update" />
        <activity android:name=".Verification_page" />
        <activity android:name=".splash_screen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Signup_page" />
        <activity android:name=".Login_page" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>